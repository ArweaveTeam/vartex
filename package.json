{
  "name": "vartex",
  "version": "1.0.0-beta0",
  "main": "dist/src/gateway",
  "repository": "git@github.com:ArweaveTeam/gateway.git",
  "author": "Arweave <hello@arweave.org>",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "vartex": "./cli/vartex.mjs"
  },
  "prettier": {
    "singleQuote": false,
    "doubleQuote": true
  },
  "execMap": {
    "ts": "node --loader ts-node/esm"
  },
  "exports": {
    ".": "./cli/vartex.mjs"
  },
  "scripts": {
    "test": "yarn run gql && TS_NODE_FILES=true node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose false --ci --forceExit --runInBand --detectOpenHandles ./test/integration2.test.ts",
    "lint": "eslint 'src/**/*.ts'",
    "gql": "node_modules/@graphql-codegen/cli/bin.js --config codegen.yml",
    "prestart": "yarn run gql && yarn run migrate:init",
    "start": "yarn run prestart && TS_NODE_FILES=true node --experimental-specifier-resolution=node --max-old-space-size=4096 --loader ts-node/esm.mjs src/gateway.ts",
    "start:gw-node": "yarn run gql && VARTEX_GW_NODE=1 TS_NODE_FILES=true node --experimental-specifier-resolution=node --max-old-space-size=4096 --loader ts-node/esm.mjs src/gateway.ts",
    "start:prod": "yarn run prestart && node start.cjs",
    "cache:purge": "rm -rf cache/imports",
    "migrate:init": "node cassandra/init.cjs",
    "prepare": "husky install"
  },
  "resolutions": {
    "cassandra-driver/long": "^4.0.0",
    "**/graphql": "16"
  },
  "dependencies": {
    "ansi-styles": "^6.1.0",
    "apollo-server-express": "^3.6.4",
    "arweave": "^1.10.23",
    "arweave-bundles": "^1.0.3",
    "base64-stream": "^1.0.0",
    "body-parser": "^1.19.2",
    "cassandra-driver": "^4.6.3",
    "colors": "^1.4.0",
    "commander": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.0",
    "event-stream": "^4.0.1",
    "exit-hook": "^3.0.0",
    "express": "^5.0.0-alpha.8",
    "express-pg-session": "^1.1.0",
    "express-session": "^1.17.2",
    "forever-monitor": "^3.0.3",
    "fs-jetpack": "^4.3.1",
    "gauge": "^4.0.3",
    "git-rev-sync": "^3.0.2",
    "got": "^12.0.2",
    "graceful-fs": "^4.2.9",
    "graphql": "16",
    "graphql-bigint": "^1.0.0",
    "graphql-fields": "^2.0.3",
    "graphql-playground-middleware-express": "^1.7.23",
    "js-sha256": "^0.9.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "p-limit": "^4.0.0",
    "p-min-delay": "^4.0.1",
    "p-queue": "^7.2.0",
    "p-wait-for": "^4.1.0",
    "p-whilst": "^3.0.0",
    "rambda": "^7.0.3",
    "ramda": "^0.28.0",
    "random-weighted-choice": "^0.1.4",
    "rfc4648": "^1.5.1",
    "rimraf": "^3.0.2",
    "shortid": "^2.2.16",
    "stream-chain": "^2.2.5",
    "stream-json": "^1.7.4",
    "ts-node": "^10.7.0",
    "winston": "^3.6.0",
    "zod": "^3.14.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-resolvers": "2.6.1",
    "@types/event-stream": "^4.0.0",
    "@types/express": "^4.17.13",
    "@types/graphql-fields": "^1.3.4",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/mime-types": "^2.1.1",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.21",
    "@types/node-cron": "^3.0.1",
    "@types/ramda": "^0.28.1",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "mock-socket": "^9.1.2",
    "nock": "^13.2.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "yall-scripts": "^1.2.0"
  }
}
